             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:controls="clr-namespace:PDFViewerSample.Controls"
             xmlns:utils="clr-namespace:PDFViewerSample.Utils"
             x:Class="PDFViewerSample.Views.GoogleDriveViewerPage"
             Title="Google Drive Viewer">
    
    <ContentPage.Content>
        <controls:GoogleDriveViewerWebView Uri="{x:Static utils:Statics.PdfUrl}" />
    </ContentPage.Content>
    
</ContentPage>