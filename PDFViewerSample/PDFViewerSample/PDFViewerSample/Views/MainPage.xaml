<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="PDFViewerSample.Views.MainPage"
             Title="PDF Viewer Sample"
             Visual="Material"
             Padding="16">

    <StackLayout>
        <Button Text="Launcher"
                Clicked="LauncherButtonOnClicked"/>
        
        <Button Text="Google Drive Viewer"
                Clicked="GoogleDriveViewerButtonOnClicked" />

        <Button Text="PDF.js"
                Clicked="PdfJsButtonOnClicked" />
    </StackLayout>

</ContentPage>
